<TEI>
  <teiHeader></teiHeader>
  <text>
    <body>
      <div type="poem">
        <lg type="strophe" n="I">
          <l n="1">
            <seg type="hemistich">Horum sermo vacuus </seg>
            <seg type="hemistich"> sensus peregrini</seg>
          </l>
          <l n="2">
            <seg type="hemistich">quos et furor agitat </seg>
            <seg type="hemistich"> et libertas vini</seg>
          </l>
          <l n="3">
            <seg type="hemistich">Sed restat consulere </seg>
            <seg type="hemistich"> mentem Augustini</seg>
          </l>
          <l n="4">
            <seg type="hemistich">per quem disputatio </seg>
            <seg type="hemistich"> concedatur fini.</seg>
          </l>
        </lg>
        <lg type="strophe" n="II">
          <l n="1">
            <seg type="hemistich">quod ei qui natus est </seg>
            <seg type="hemistich"> satis adaptatur</seg>
          </l>
          <l n="2">
            <seg type="hemistich">cuiusvis potentia </seg>
            <seg type="hemistich"> per hunc obscuratur.</seg>
          </l>
        </lg>
        <lg type="strophe" n="III">
          <l n="1">
            <seg type="hemistich">Ergo cum muneribus </seg>
            <seg type="hemistich"> una procedamus</seg>
          </l>
          <l n="2">
            <seg type="hemistich">et quo stella duxerit </seg>
            <seg type="hemistich"> gressus dirigamus</seg>
          </l>
          <l n="3">
            <seg type="hemistich">ut quando viderimus </seg>
            <seg type="hemistich"> quem natus speramus</seg>
          </l>
          <l n="4">
            <seg type="hemistich">nostra ei munia </seg>
            <seg type="hemistich"> reges offeramus.</seg>
          </l>
        </lg>
        <lg type="strophe" n="IV">
          <l n="1">Ubi est qui natus est etc.</l>
        </lg>
        <lg type="strophe" n="V">
          <l n="1">
            <seg type="hemistich">Vos qui regum habitus </seg>
            <seg type="hemistich"> et insigne geritis</seg>
          </l>
          <l n="2">
            <seg type="hemistich">nobis notum facite </seg>
            <seg type="hemistich"> quare sic inceditis</seg>
          </l>
          <l n="3">
            <seg type="hemistich">vel si noturum aliquid </seg>
            <seg type="hemistich"> reserandum noscitis</seg>
          </l>
          <l n="4">
            <seg type="hemistich">quod ad aures [...] </seg>
            <seg type="hemistich"> regis ferre queritis.</seg>
          </l>
        </lg>
        <lg type="strophe" n="VI">
          <l n="1">
            <seg type="hemistich">Nos Herodis vernule </seg>
            <seg type="hemistich"> sumus et vicarii</seg>
          </l>
          <l n="2">
            <seg type="hemistich">ad quem sepe transvolant </seg>
            <seg type="hemistich"> ex diversis nuntii</seg>
          </l>
          <l n="3">
            <seg type="hemistich">nulla nobis clausa sunt </seg>
            <seg type="hemistich"> secreta palatii</seg>
          </l>
          <l n="4">
            <seg type="hemistich">ergo scire possumus </seg>
            <seg type="hemistich"> vestri rem negotii.</seg>
          </l>
        </lg>
        <lg type="strophe" n="VII">
          <l n="1">
            <seg type="hemistich">Sepelire nolumus </seg>
            <seg type="hemistich"> quod a nobis queritur</seg>
          </l>
          <l n="2">
            <seg type="hemistich">ipsum stella reserat </seg>
            <seg type="hemistich"> que a nobis cernitur</seg>
          </l>
          <l n="3">regem natum querimus de quo stella loquitur</l>
          <l n="4">
            <seg type="hemistich">quod eius imperium </seg>
            <seg type="hemistich"> nullo fine clauditur.</seg>
          </l>
        </lg>
        <lg type="strophe" n="VIII">
          <l n="1">
            <seg type="hemistich">Felix istud veniet </seg>
            <seg type="hemistich"> Herodi preconium</seg>
          </l>
          <l n="2">
            <seg type="hemistich">et libenter audiet </seg>
            <seg type="hemistich"> hoc de rege nuntium</seg>
          </l>
          <l n="3">
            <seg type="hemistich">ut hinc ergo primitus </seg>
            <seg type="hemistich"> per nos sumat gaudium</seg>
          </l>
          <l n="4">
            <seg type="hemistich">vos nostrum sequimini </seg>
            <seg type="hemistich"> [...] vestigium.</seg>
          </l>
        </lg>
        <lg type="strophe" n="IX">
          <l n="1">
            <seg type="hemistich">Rex Herodes accipe </seg>
            <seg type="hemistich"> quiddam ammirandum</seg>
          </l>
          <l n="2">
            <seg type="hemistich">iam a tribus regibus </seg>
            <seg type="hemistich"> tibi reserandum</seg>
          </l>
          <l n="3">
            <seg type="hemistich">ipsi natum asserunt </seg>
            <seg type="hemistich"> regem venerandum</seg>
          </l>
          <l n="4">cui esse non ambigunt orbem subiugandum.</l>
        </lg>
        <lg type="strophe" n="X">
          <l n="1">
            <seg type="hemistich">Cur audetis talia </seg>
            <seg type="hemistich"> regi presentare</seg>
          </l>
          <l n="2">
            <seg type="hemistich">nolite vos consulo </seg>
            <seg type="hemistich"> falsum fabricare</seg>
          </l>
          <l n="3">
            <seg type="hemistich">num Herodes ego sum </seg>
            <seg type="hemistich"> potens subiugare</seg>
          </l>
          <l n="4">
            <seg type="hemistich">quicquid mundus continet </seg>
            <seg type="hemistich"> celum terra[m] mare.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XI">
          <l n="1">
            <seg type="hemistich">Huc Iudea veniat </seg>
            <seg type="hemistich"> fecunda consilio</seg>
          </l>
          <l n="2">
            <seg type="hemistich">ut nobiscum disserat </seg>
            <seg type="hemistich"> super hoc negocio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">et ego vos precipiam </seg>
            <seg type="hemistich"> exponi suplicio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">si vos esse devios </seg>
            <seg type="hemistich"> conprobabit ratio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XII">
          <l n="1">
            <seg type="hemistich">Te magister alloquor </seg>
            <seg type="hemistich"> et advertant alii</seg>
          </l>
          <l n="2">
            <seg type="hemistich">nostra mordet viscera </seg>
            <seg type="hemistich"> duri fama nuntii</seg>
          </l>
          <l n="3">
            <seg type="hemistich">huc tres magi veniunt </seg>
            <seg type="hemistich"> non astrorum inscii</seg>
          </l>
          <l n="4">
            <seg type="hemistich">qui ad ortum properant </seg>
            <seg type="hemistich"> prepotentis filii.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XIII">
          <l n="1">
            <seg type="hemistich">Ne curarum domine </seg>
            <seg type="hemistich"> verseris in bivio</seg>
          </l>
          <l n="2">
            <seg type="hemistich">tres huc reges veniant </seg>
            <seg type="hemistich"> querendo de filio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">quibus te concilies </seg>
            <seg type="hemistich"> diligenti studio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">et eis sic loquere </seg>
            <seg type="hemistich"> sub amoris pallio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XIV">
          <l n="1">
            <seg type="hemistich">Reges estis video </seg>
            <seg type="hemistich"> quod prophetat habitus</seg>
          </l>
          <l n="2">
            <seg type="hemistich">vester michi gratus est </seg>
            <seg type="hemistich"> factus ad nos transitus</seg>
          </l>
          <l n="3">
            <seg type="hemistich">Sed quid vos huc traxerit </seg>
            <seg type="hemistich"> reserate penitus</seg>
          </l>
          <l n="4">
            <seg type="hemistich">nam vobis ad omnia </seg>
            <seg type="hemistich"> rex erit expositus.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XV">
          <l n="1">
            <seg type="hemistich">Stella nova radiat </seg>
            <seg type="hemistich"> eius ortus nuntia</seg>
          </l>
          <l n="2">
            <seg type="hemistich">cui mundus obediet </seg>
            <seg type="hemistich"> et qui reget omnia</seg>
          </l>
          <l n="3">
            <seg type="hemistich">et nil stare poterit </seg>
            <seg type="hemistich"> absque huius gratia</seg>
          </l>
          <l n="4">
            <seg type="hemistich">nos ad illum tendimus </seg>
            <seg type="hemistich"> hec ferentes munia.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XVI">
          <l n="1">
            <seg type="hemistich">Ne sim vos impediens </seg>
            <seg type="hemistich"> ad vie propositum</seg>
          </l>
          <l n="2">
            <seg type="hemistich">ite ad nos postea </seg>
            <seg type="hemistich"> maturantes reditum</seg>
          </l>
          <l n="3">
            <seg type="hemistich">ut et ego veniens </seg>
            <seg type="hemistich"> munus feram debitum</seg>
          </l>
          <l n="4">
            <seg type="hemistich">ei cui non ambigo </seg>
            <seg type="hemistich"> mundum fore subditum.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XVII">
          <l n="1">
            <seg type="hemistich">Magnum vobis gaudium </seg>
            <seg type="hemistich"> pastores annuntio</seg>
          </l>
          <l n="2">
            <seg type="hemistich">deus se circumdedit </seg>
            <seg type="hemistich"> carnis vestre pallio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">quem non peperit </seg>
            <seg type="hemistich"> carnali conmercio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">immo virgo permanens </seg>
            <seg type="hemistich"> mater est ex filio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XVIII">
          <l n="1">
            <seg type="hemistich">Tu ne credas talibus </seg>
            <seg type="hemistich"> pastorum simplicitas</seg>
          </l>
          <l n="2">
            <seg type="hemistich">sciat esse frivola </seg>
            <seg type="hemistich"> que non probat veritas</seg>
          </l>
          <l n="3">
            <seg type="hemistich">quod sic in presepio </seg>
            <seg type="hemistich"> sit sepulta deitas</seg>
          </l>
          <l n="4">
            <seg type="hemistich">nimis est ad oculum </seg>
            <seg type="hemistich"> reserata falsitas.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XIX">
          <l n="1">
            <seg type="hemistich">Pastores querite </seg>
            <seg type="hemistich"> natum in presepio</seg>
          </l>
          <l n="2">
            <seg type="hemistich">et votum solvite </seg>
            <seg type="hemistich"> matri cum filio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">nec mora veniat </seg>
            <seg type="hemistich"> isti consilio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">sed vos huc dirigat </seg>
            <seg type="hemistich"> mentis devotio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XX">
          <l n="1">
            <seg type="hemistich">Simplex cetus aspice </seg>
            <seg type="hemistich"> qualis astucia</seg>
          </l>
          <l n="2">
            <seg type="hemistich">eius qui sic fabricat </seg>
            <seg type="hemistich"> vero contraria</seg>
          </l>
          <l n="3">
            <seg type="hemistich">utque sua falleret </seg>
            <seg type="hemistich"> nugis mendatia</seg>
          </l>
          <l n="4">
            <seg type="hemistich">in ritmis conciliat </seg>
            <seg type="hemistich"> que profert omnia.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXI">
          <l n="1">
            <seg type="hemistich">Nunquid frater colligit </seg>
            <seg type="hemistich"> ea que audio?</seg>
          </l>
          <l n="2">
            <seg type="hemistich">Quedam vox insinuat </seg>
            <seg type="hemistich"> de nato filio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">verum in contrarium </seg>
            <seg type="hemistich"> ab hoc suscipio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">quod audita resident </seg>
            <seg type="hemistich"> iuncta mendatio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXII">
          <l n="1">
            <seg type="hemistich">Cur non aures vertitis </seg>
            <seg type="hemistich"> ad hunc veri nuntium</seg>
          </l>
          <l n="2">
            <seg type="hemistich">quis est iste subdolus </seg>
            <seg type="hemistich"> vertens vos in devium?</seg>
          </l>
          <l n="3">
            <seg type="hemistich">Ne vos error induat </seg>
            <seg type="hemistich"> propter adversarium</seg>
          </l>
          <l n="4">
            <seg type="hemistich">ite nam quod predico </seg>
            <seg type="hemistich"> monstrabit presepium.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXIII">
          <l n="1">
            <seg type="hemistich">O gens simplex nimium </seg>
            <seg type="hemistich"> et in sensu vulnerata</seg>
          </l>
          <l n="2">
            <seg type="hemistich">fert fenum et pabulum </seg>
            <seg type="hemistich"> que bubus non ingrata</seg>
          </l>
          <l n="3">
            <seg type="hemistich">in presepi comedat </seg>
            <seg type="hemistich"> deitas reclinata</seg>
          </l>
          <l n="4">
            <seg type="hemistich">debacharis numium </seg>
            <seg type="hemistich"> cum putas ista rata.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXIV">
          <l n="1">
            <seg type="hemistich">Audi frater iterum </seg>
            <seg type="hemistich"> qualis repugnantia</seg>
          </l>
          <l n="2">
            <seg type="hemistich">inde quedam audio </seg>
            <seg type="hemistich"> hinc horum contraria</seg>
          </l>
          <l n="3">
            <seg type="hemistich">meus simplex animus </seg>
            <seg type="hemistich"> mea mens non sobria</seg>
          </l>
          <l n="4">
            <seg type="hemistich">ignorat que potior </seg>
            <seg type="hemistich"> sit horum sententia.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXV">
          <l n="1">Nolite redire ad Herodem etc.</l>
        </lg>
        <lg type="strophe" n="XXVI">
          <l n="1">
            <seg type="hemistich">Gens Iudea properet </seg>
            <seg type="hemistich"> ut Herodem audiat</seg>
          </l>
          <l n="2">
            <seg type="hemistich">et prestet consilium </seg>
            <seg type="hemistich"> de re que me sauciat.</seg>
          </l>
          <l n="3">
            <seg type="hemistich">Rex Herodes anxius </seg>
            <seg type="hemistich"> ignorat quid faciat,</seg>
          </l>
          <l n="4">
            <seg type="hemistich">cum a tribus regibus </seg>
            <seg type="hemistich"> se lusum inspiciat.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXVII">
          <l n="1">
            <seg type="hemistich">Tu magister aperi </seg>
            <seg type="hemistich"> prophetarum edita</seg>
          </l>
          <l n="2">
            <seg type="hemistich">si qua sunt de puero </seg>
            <seg type="hemistich"> a prophetis tradita</seg>
          </l>
          <l n="3">
            <seg type="hemistich">nam a te fideliter </seg>
            <seg type="hemistich"> re michi exposita</seg>
          </l>
          <l n="4">
            <seg type="hemistich">se monstrabunt proprii </seg>
            <seg type="hemistich"> cordis [...] abscondita.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXVIII">
          <l n="1">
            <seg type="hemistich">Ite ite pariter </seg>
            <seg type="hemistich"> manu iuncta gladio</seg>
          </l>
          <l n="2">
            <seg type="hemistich">etas adhuc tenera </seg>
            <seg type="hemistich"> nulli parcat filio</seg>
          </l>
          <l n="3">
            <seg type="hemistich">immo mater quelibet </seg>
            <seg type="hemistich"> nudo fleat gremio</seg>
          </l>
          <l n="4">
            <seg type="hemistich">ut de nato puero </seg>
            <seg type="hemistich"> michi detur ultio.</seg>
          </l>
        </lg>
        <lg type="strophe" n="XXIX">
          <l n="1">Heu heu heu</l>
          <l n="2">mens Herodis effera</l>
          <l n="3">cur in nostra viscera</l>
          <l n="4">bella movet aspera?</l>
        </lg>
        <lg type="strophe" n="XXX">
          <l n="1">Heu heu heu</l>
          <l n="2">que etas adhuc tenera</l>
          <l n="3">sugens ubera</l>
          <l n="4">perpetravit scelera?</l>
        </lg>
        <lg type="strophe" n="XXXI">
          <l n="1">Heu heu heu</l>
          <l n="2">iste dolor anxius</l>
          <l n="3">dum transegit inpius</l>
          <l n="4">innocentes gladius.</l>
        </lg>
        <lg type="strophe" n="XXXII">
          <l n="1">Heu heu heu</l>
          <l n="2">proles adhuc tenera</l>
          <l n="3">per te mater misera</l>
          <l n="4">descendet ad infera.</l>
        </lg>
        <lg type="strophe" n="XXXIII">
          <l n="1">Heu heu</l>
          <l n="2">michi vite gaudium</l>
          <l n="3">fili nunc supplicium</l>
          <l n="4">mortis eris ostium.</l>
        </lg>
        <lg type="strophe" n="XXXIV">
          <l n="1">Accipe matrem et filium et vade in Egiptum.</l>
        </lg>
        <lg type="strophe" n="XXXV">
          <l n="1">Omnia dura pati</l>
          <l n="2">vitando pericula nati</l>
          <l n="3">Mater sum presto</l>
          <l n="4">iam vadam tu comes esto.</l>
        </lg>
      </div>
    </body>
  </text>
</TEI>