      <div type="poem">
        <lg type="strophe" n="I">
          <l n="1">Vite perdite</l>
          <l n="2">
            <seg type="hemistich">me legi</seg>
            <seg type="hemistich">subdideram</seg>
          </l>
          <l n="3">minus licite</l>
          <l n="4">
            <seg type="hemistich">dum fregi</seg>
            <seg type="hemistich">quod voveram</seg>
          </l>
          <l n="5">
            <seg type="hemistich">sed ad vite vesperam</seg>
            <seg type="hemistich">corrigendum legi</seg>
          </l>
          <l n="6">
            <seg type="hemistich">quicquid ante perperam</seg>
            <seg type="hemistich">puerilis egi</seg>
          </l>
        </lg>
