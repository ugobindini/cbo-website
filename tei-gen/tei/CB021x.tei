<TEI>
  <teiHeader></teiHeader>
  <text>
    <body>
      <div type="poem">
        <lg type="strophe" n="I">
          <l n="1">Presens dies expendatur</l>
          <l n="2">in eius preconium,</l>
          <l n="3">cuius virtus dilatatur</l>
          <l n="4">in ore laudantium,</l>
          <l n="5">si gestorum teneatur</l>
          <l n="6">finis et inicium.</l>
        </lg>
        <lg type="strophe" n="II">
          <l n="1">Verbo vite roboratus</l>
          <l n="2">prosiliit Porfirius</l>
          <l n="3">cum ducentis decollatus</l>
          <l n="4">migrat palme socius,</l>
          <l n="5">Katerine cruciatus</l>
          <l n="6">maturat Maxentius.</l>
        </lg>
        <lg type="strophe" n="III">
          <l n="1">Imminente passione...</l>
        </lg>
        <lg type="strophe" n="IV">
          <l n="1">Hoc declarat, hoc exemplat...</l>
        </lg>
        <lg type="strophe" n="V">
          <l n="1">Cum doloris corporalis...</l>
        </lg>
        <lg type="strophe" n="VI">
          <l n="1">Gloria et honor...</l>
        </lg>
      </div>
    </body>
  </text>
</TEI>