  <text>
    <body>
      <div type="poem" met="6pp+6p/6pp+6p/6pp+6p/6pp+6p/6pp+6p/6pp+6p" rhyme="a+b/a+b/c+d/c+d/c+d/c+d">
        <lg type="strophe" n="I">
          <l n="1">
            <seg type="hemistich">
              <w>Celum</w>
              <pc resp="#editor">,</pc>
              <w>non</w>
              <w>animum</w>
            </seg>
            <seg type="hemistich">
              <w>mutat</w>
              <w>stabilitas</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <w>firmans</w>
              <w>id</w>
              <w>optimum</w>
              <pc resp="#editor">,</pc>
            </seg>
            <seg type="hemistich">
              <w>quod</w>
              <w>mentis</w>
              <w>firmitas</w>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <app type="text">
                <lem wit="cod. HS"><w>vovet</w></lem>
                <rdg wit="V T"><w>prebet</w></rdg>
              </app>
              <w>cum</w>
              <w>animi</w>
            </seg>
            <seg type="hemistich">
              <w>tamen</w>
              <w>iuditio</w>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <w>Nam</w>
              <w>si</w>
              <pc resp="#editor">,</pc>
              <w>turpissimi</w>
            </seg>
            <seg type="hemistich">
              <w>voti</w>
              <w>consilio</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="5">
            <seg type="hemistich">
              <w>vis</w>
              <w>scelus</w>
              <w>imprimi</w>
            </seg>
            <seg type="hemistich">
              <w>facto</w>
              <w>nefario</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="6">
            <seg type="hemistich">
              <w>debet</w>
              <w>hec</w>
              <w>perimi</w>
            </seg>
            <seg type="hemistich">
              <w>facta</w>
              <w>promissio</w>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
        </lg>
        <lg type="strophe" n="II">
          <l n="1">
            <seg type="hemistich">
              <w>Non</w>
              <w>erat</w>
              <w>stabilis</w>
            </seg>
            <seg type="hemistich">
              <w>gradus</w>
              <w>qui</w>
              <w>cecidit</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <w>pes</w>
              <w>eius</w>
              <w>labilis</w>
            </seg>
            <seg type="hemistich">
              <w>domus</w>
              <w>que</w>
              <app type="text">
                <lem wit="cod. HS"><w>occidit</w></lem>
                <rdg wit="V T"><w>corruit</w></rdg>
              </app>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <w>Hinc</w>
              <w>tu</w>
              <w>considera</w>
            </seg>
            <seg type="hemistich">
              <w>quid</w>
              <w>agi</w>
              <w>censeas</w>
              <pc resp="#editor">:</pc>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <w>dum</w>
              <w>res</w>
              <w>est</w>
              <w>libera</w>
            </seg>
            <seg type="hemistich">
                  <w>sic</w>
                  <w>sta</w>
                  <w>ne</w>
                  <w>iaceas</w>
                  <pc resp="#editor">.</pc>
            </seg>
          </l>
          <l n="5">
            <seg type="hemistich">
              <w>Prius</w>
              <w>delibera</w>
            </seg>
            <seg type="hemistich">
              <w>quod</w>
              <w>factum</w>
              <w>subeas</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="6">
            <seg type="hemistich">
              <w>ne</w>
              <w>die</w>
              <w>postera</w>
            </seg>
            <seg type="hemistich">
              <w>sero</w>
              <w>peniteas</w>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
        </lg>
        <lg type="strophe" n="III">
          <l n="1">
            <seg type="hemistich">
              <app type="text">
                <lem wit="cod. HS T"><w>Facti</w></lem>
                <rdg wit="V"><w>Cepti</w></rdg>
              </app>
              <w>dimidium</w>
            </seg>
            <seg type="hemistich">
              <w>habet</w>
              <pc resp="#editor">,</pc>
              <w>qui</w>
              <w>ceperit</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <w>ceptum</w>
              <w>negocium</w>
            </seg>
            <seg type="hemistich">
              <w>si</w>
              <w>non</w>
              <w>omiserit</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <w>non</w>
              <w>tantum</w>
              <w>deditus</w>
            </seg>
            <seg type="hemistich">
              <w>circa</w>
              <w>principia</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <app type="text">
                <lem wit="cod. HS T"><w>nedum</w></lem>
                <rdg wit="V"><w>verum</w></rdg>
              </app>
              <w>sollicitus</w>
            </seg>
            <seg type="hemistich">
              <w>pro</w>
              <w>finis</w>
              <w>gloria</w>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
          <l n="5">
            <seg type="hemistich">
              <w>Nam</w>
              <w>rerum</w>
              <w>exitus</w>
            </seg>
            <seg type="hemistich">
              <w>librat</w>
              <w>industria</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="6">
            <seg type="hemistich">
              <w>subit</w>
              <w>introitus</w>
            </seg>
            <seg type="hemistich">
              <w>preceps</w>
              <w>incuria</w>
              <pc>&#183;</pc>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
        </lg>
