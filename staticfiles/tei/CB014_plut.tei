<TEI>
  <teiHeader />
  <text>
    <body>
      <div type="poem" met="4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp" rhyme="a+b/a+b/b+a/c+d/c+d/d+c/e+f/e+f">
        <lg type="strophe">
          <l n="1">
            <seg type="hemistich">
              <w>O</w>
              <w>varium</w>
            </seg>
            <seg type="hemistich">
              <w>Fortune</w>
              <w>lubricum</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <app type="text">
                <lem wit="HS V T"><w>dans</w></lem>
                <rdg wit="cod."><w>dat</w> corr. ex <w>dan</w></rdg>
              </app>
              <w>dubium</w>
            </seg>
            <seg type="hemistich">
              <w>tribunal</w>
              <w>iudicum</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <w>non</w>
              <w>modicum</w>
            </seg>
            <seg type="hemistich">
              <w>paras</w>
              <w>huic</w>
              <w>premium</w>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <w>quem</w>
              <w>colere</w>
            </seg>
            <seg type="hemistich">
              <w>tua</w>
              <w>vult</w>
              <w>gratia</w>
              <pc>&#183;</pc>
            </seg>
          </l>
          <l n="5">
            <seg type="hemistich">
              <w>et</w>
              <w>petere</w>
            </seg>
            <seg type="hemistich">
              <w>rote</w>
              <w>sublimia</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="6">
            <seg type="hemistich">
              <w>dans</w>
              <app type="text">
                <lem wit="cod."><w>premia</w></lem>
                <rdg wit="HS V T"><w>dubia</w></rdg>
              </app>
            </seg>
            <seg type="hemistich">
              <w>tamen</w>
              <pc resp="#editor">,</pc>
              <w>prepostere</w>
              <pc>&#183;</pc>
            </seg>
          </l>
          <l n="7">
            <seg type="hemistich">
              <w>de</w>
              <w>stercore</w>
            </seg>
            <seg type="hemistich">
              <w>pauperem</w>
              <w>erigens</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="8"><seg type="hemistich">
            <w>de</w>
            <app type="text">
              <lem wit="HS V T"><w>rhetore</w></lem>
              <rdg wit="cod."><w>recthore</w></rdg>
            </app>
          </seg>
            <seg type="hemistich">
              <w>consulem</w>
              <w>eligens</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
        </lg>
      </div>
    </body>
  </text>
