    <body>
      <div type="poem" met="6pp/6pp/6pp/6pp/8p/8p/8p/8p/8pp/7p/8pp/7p" rhyme="a/a/a/a/b/b/c/c/d/e/d/e">
        <lg type="strophe" n="I">
          <l n="1">
            <w>Dic</w>
            <pc resp="#editor">,</pc>
            <w>Christi</w>
            <w>veritas</w>
            <pc resp="#editor">,</pc>
          </l>
          <l n="2">
            <w>dic</w>
            <pc resp="#editor">,</pc>
            <app type="text">
              <lem wit="cod.">
                <w>rara</w>
                <w>caritas</w>
              </lem>
              <rdg wit="HS V T"><w>cara</w><w>raritas</w></rdg>
            </app>
          </l>
          <l n="3">
            <w>dic</w>
            <pc resp="#editor">,</pc>
            <app type="text">
              <lem wit="cod.">
                <w>cara</w>
                <w>raritas</w>
              </lem>
              <rdg wit="HS V T"><w>rara</w><w>caritas</w></rdg>
              <note><w>raritas</w> corr. ex <w>caritas</w> cod.</note>
            </app>
            <pc resp="#editor">,</pc>
          </l>
          <l n="4">
            <w>ubi</w>
            <w>nunc</w>
            <w>habitas</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="5">
            <w>Aut</w>
            <w>in</w>
            <w>valle</w>
            <w>visionis</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="6">
            <w>aut</w>
            <w>in</w>
            <w>throno</w>
            <w>Pharaonis</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="7">
            <w>aut</w>
            <w>in</w>
            <w>alto</w>
            <w>cum</w>
            <w>Nerone</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="8">
            <w>aut</w>
            <w>in</w>
            <w>antro</w>
            <w>cum</w>
            <w>Theone</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="9">
            <w>Vel</w>
            <w>in</w>
            <w>fiscella</w>
            <w>scirpea</w>
          </l>
          <l n="10">
            <w>cum</w>
            <w>Moyse</w>
            <w>plorante</w>
            <pc resp="#editor">?</pc>
          </l>
          <l n="11">
            <w>Vel</w>
            <w>in</w>
            <w>domo</w>
            <app type="text">
              <lem wit="HS V T">
                <w>Romulea</w>
              </lem>
              <rdg wit="cod."><w>rumulea</w></rdg>
            </app>
            <pc resp="#editor">,</pc>
          </l>
          <l n="12">
            <w>cum</w>
            <w>bulla</w>
            <w>fulminante</w>
            <pc resp="#editor">?</pc>
          </l>
