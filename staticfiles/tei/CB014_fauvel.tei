  <text>
    <body>
      <div type="poem" met="4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/6pp/4pp+6pp/4p+6pp/4pp+6pp/4pp+6pp" rhyme="a+b/a+b/b+a/c+d/c+d/c/e/e/f/f">
        <lg type="strophe">
          <l n="1">
            <seg type="hemistich">O varium</seg>
            <seg type="hemistich">fortune lubricum</seg>
          </l>
          <l n="2">
            <seg type="hemistich">dans dubium</seg>
            <seg type="hemistich">tribunal iudicum</seg>
          </l>
          <l n="3">
            <seg type="hemistich">non modicum</seg>
            <seg type="hemistich">panis huic premium</seg>
          </l>
          <l n="4">
            <seg type="hemistich">quem colere</seg>
            <seg type="hemistich">vult tua gracia</seg>
          </l>
          <l n="5">
            <seg type="hemistich">et petere</seg>
            <seg type="hemistich">rote sublimia</seg>
          </l>
          <l n="6">tamen prepostere</l>
          <l n="7">
            <seg type="hemistich">de stercore</seg>
            <seg type="hemistich">pauperem erigens</seg>
          </l>
          <l n="8">
            <seg type="hemistich">Et Fauvellum</seg>
            <seg type="hemistich">in altum erigens</seg>
          </l>
          <l n="9">
            <seg type="hemistich">quo consule</seg>
            <seg type="hemistich">fides est mortua</seg>
          </l>
          <l n="10">
            <seg type="hemistich">ecclesia</seg>
            <seg type="hemistich">ductore vidua</seg>
          </l>
        </lg>
      </div>
    </body>
  </text>
