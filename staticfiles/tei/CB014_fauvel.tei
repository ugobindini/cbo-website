<TEI>
  <teiHeader />
  <text>
    <body>
      <div type="poem" met="4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/4pp+6pp/6pp/4pp+6pp/4p+6pp/4pp+6pp/4pp+6pp" rhyme="a+b/a+b/b+a/c+d/c+d/c/e/e/f/f">
        <lg type="strophe" n="I">
          <l n="1">
            <seg type="hemistich">
              <w>O</w>
              <w>varium</w>
            </seg>
            <seg type="hemistich">
              <w>Fortune</w>
              <w>lubricum</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <w>dans</w>
              <w>dubium</w>
            </seg>
            <seg type="hemistich">
              <w>tribunal</w>
              <app type="text">
                <lem wit="HS V T"><w>iudicum</w></lem>
                <rdg wit="cod."><w>iudicium</w></rdg>
              </app>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <w>non</w>
              <w>modicum</w>
            </seg>
            <seg type="hemistich">
              <app type="text">
                <lem wit="HS T"><w>paras</w></lem>
                <rdg wit="cod."><w>panis</w></rdg>
                <rdg wit="V"><w>parans</w></rdg>
              </app>
              <w>huic</w>
              <w>premium</w>
              <pc>&#183;</pc>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <w>quem</w>
              <w>colere</w>
            </seg>
            <seg type="hemistich">
              <app type="text">
                <lem wit="cod.">
                  <w>vult</w>
                  <w>tua</w>
                </lem>
                <rdg wit="HS V T"><w>tua</w><w>vult</w></rdg>
              </app>
              <w>gracia</w>
            </seg>
          </l>
          <l n="5">
            <seg type="hemistich">
              <w>et</w>
              <w>petere</w>
            </seg>
            <seg type="hemistich">
              <w>rote</w>
              <w>sublimia</w>
              <pc>&#183;</pc>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="6">
            <app type="text">
              <lem><w>tamen</w></lem>
              <rdg wit="HS V T"><w>dans</w><w>dubia</w><w>tamen</w></rdg>
            </app>
            <w>prepostere</w>
          </l>
          <l n="7">
            <seg type="hemistich">
              <w>de</w>
              <w>stercore</w>
            </seg>
            <seg type="hemistich">
              <w>pauperem</w>
              <app type="text">
                <lem>
                  <w>erigens</w>
                </lem>
                <note>fort. <w>eligens</w></note>
              </app>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="8">
            <seg type="hemistich">
              <w>Et</w>
              <w>Fauvellum</w>
            </seg>
            <seg type="hemistich">
              <w>in</w>
              <w>altum</w>
              <w>erigens</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="9">
            <seg type="hemistich">
              <w>quo</w>
              <w>consule</w>
            </seg>
            <seg type="hemistich">
              <w>fides</w>
              <w>est</w>
              <w>mortua</w>
              <pc resp="#editor">,</pc>
            </seg>
          </l>
          <l n="10">
            <seg type="hemistich">
              <w>Ecclesia</w>
            </seg>
            <seg type="hemistich">
              <w>ductore</w>
              <w>vidua</w>
            </seg>
          </l>
        </lg>
      </div>
    </body>
  </text>
</TEI>