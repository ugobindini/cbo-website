  <text>
    <body>
      <div type="poem" met="6pp+6pp/6pp+6pp/6pp+6pp/6pp+6pp/6pp/6pp+6pp" rhyme="a+b/a+b/a+b/a+b/b/a+b">
        <lg type="strophe">
          <l n="1">
            <seg type="hemistich">
              <w>Procurans</w>
              <w>hodium</w>
            </seg>
            <seg type="hemistich">
              <app type="text">
                <lem wit="CB F M HS V T">
                  <w>effectu</w>
                </lem>
                <rdg wit="cod."><w>effectum</w></rdg>
              </app>
              <w>proprio</w>
            </seg>
          </l>
          <l n="2">
            <seg type="hemistich">
              <w>vix</w>
              <w>detrahentium</w>
            </seg>
            <seg type="hemistich">
              <w>gaudet</w>
              <w>intencio</w>
              <pc resp="#editor">.</pc>
            </seg>
          </l>
          <l n="3">
            <seg type="hemistich">
              <w>Nexus</w>
              <w>est</w>
              <w>cordium</w>
            </seg>
            <seg type="hemistich">
              <w>ipsa</w>
              <w>detractio</w>
              <pc resp="#editor">:</pc>
            </seg>
          </l>
          <l n="4">
            <seg type="hemistich">
              <w>sic</w>
              <w>per</w>
              <w>contrarium</w>
            </seg>
            <seg type="hemistich">
              <w>ab</w>
              <w>hoste</w>
              <w>nescio</w>
            </seg>
          </l>
          <l n="5">
            <w>fit</w>
            <app type="text">
              <lem wit="CB F M HS V T">
                <w>hic</w>
              </lem>
              <rdg wit="cod."><w>hec</w></rdg>
            </app>
            <w>provisio</w>
            <pc resp="#editor">:</pc>
          </l>
          <l n="6">
            <seg type="hemistich">
              <w>in</w>
              <w>hoc</w>
              <w>amancium</w>
            </seg>
            <seg type="hemistich">
              <w>felix</w>
              <w>conditio</w>
              <pc resp="#editor">!</pc>
            </seg>
          </l>
        </lg>
      </div>
