  <text>
    <body>
      <div type="poem" met="8pp/8pp/8pp/8pp/8pp/4pp/8pp/8pp/8pp/8pp" rhyme="a/b/b/b/c/a/c/d/d/c">
        <lg type="strophe" n="I">
          <l n="1">
            <w>Non</w>
            <w>te</w>
            <w>lusisse</w>
            <w>pudeat</w>
            <pc>&#183;</pc>
            <pc resp="#editor">,</pc>
          </l>
          <l n="2">
            <w>sed</w>
            <w>ludum</w>
            <w>non</w>
            <w>incidere</w>
            <pc>&#183;</pc>
          </l>
          <l n="3">
            <w>et</w>
            <w>que</w>
            <w>lusisti</w>
            <w>temere</w>
            <pc resp="#editor">,</pc>
          </l>
          <l n="4">
            <w>ad</w>
            <w>vite</w>
            <w>frugem</w>
            <w>vertere</w>
            <pc resp="#editor">.</pc>
          </l>
          <l n="5">
            <w>Magistra</w>
            <w>morum</w>
            <app type="text">
              <lem wit="HS V W T"><w>doceat</w></lem>
              <rdg wit="cod."><w>ratio</w></rdg>
            </app>
          </l>
          <l n="6">
            <w>te</w>
            <app type="text">
              <lem wit="HS V W T"><w>ratio</w></lem>
              <rdg wit="cod."><w>doceat</w></rdg>
            </app>
            <pc>&#183;</pc>
            <pc resp="#editor">,</pc>
          </l>
          <l n="7">
            <w>ut</w>
            <w>dignus</w>
            <w>pontificio</w>
          </l>
          <l n="8">
            <w>divini</w>
            <w>dono</w>
            <w>numinis</w>
            <pc resp="#editor">,</pc>
          </l>
          <l n="9">
            <w>ad</w>
            <w>laudem</w>
            <w>Christi</w>
            <w>nominis</w>
          </l>
          <l n="10">
            <w>fungaris</w>
            <w>sacerdocio</w>
            <pc>&#183;</pc>
            <pc resp="#editor">.</pc>
          </l>
        </lg>
      </div>
